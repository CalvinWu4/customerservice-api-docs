									"            \"firstName\": { \"type\": \"string\" },",
									"            \"lastName\": { \"type\": \"string\" }",
									"        }",
									"    },",
									"    \"product\": {",
									"        \"required\": [\"serialNumber\", \"name\", \"description\"],",
									"        \"properties\": {",
									"            \"serialNumber\": { \"type\": \"string\" },",
									"            \"name\": { \"type\": \"string\" },",
									"            \"description\": { \"type\": \"string\" }",
									"        }",
									"    },",
									"    \"comments\": {",
									"        \"type\": \"array\",",
									"        \"items\": {",
									"            \"type\": \"object\",",
									"            \"required\": [\"ticketId\", \"content\", \"author\", \"commentedBy\", \"dateCreated\"],",
