									"            \"firstName\": { \"type\": \"string\" },",
									"            \"lastName\": { \"type\": \"string\" },",
									"            \"email\": { \"type\": \"string\" },",
									"            \"address\": {",
									"                \"required\": [\"line1\", \"city\", \"state\", \"zipcode\"],",
									"                \"properties\": {",
									"                    \"line1\": { \"type\": \"string\" },",
									"                    \"line2\": { \"type\": \"string\" },",
									"                    \"city\": { \"type\": \"string\" },",
									"                    \"state\": { \"type\": \"string\" },",
									"                    \"zipcode\": { \"type\": \"string\" }",
									"                }",
									"            }",
									"        }",
									"    },",
									"    \"agent\": {",
									"        \"required\": [\"id\", \"firstName\", \"lastName\"],",
